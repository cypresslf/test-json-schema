{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string"
        },
        "last": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": ["first", "last"]
    },
    "age": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false,
  "required": ["name", "age"]
}
